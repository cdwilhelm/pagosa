<h1>Listing projects</h1>
<p>
<table>
  <tr>
    <th>Name</th>
    <th>Display name</th>
    <th>Description</th>
  </tr>

<% for project in @projects %>
  <tr>
    <td><%= link_to  project.name, project %></td>
    <td><%=h project.display_name %></td>
    <td><%=h project.description %></td>
  </tr>
<% end %>
</table>